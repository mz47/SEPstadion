package drawing;

import javax.inject.Named;
import javax.enterprise.context.Dependent;

/**
 *
 * @author marcel
 */
@Named(value = "spiel")
@Dependent
public class Spiel
{
    /**
     * Creates a new instance of Spiel
     */
    String Spiele;
    public Spiel()
    {
        XmlWrapper wrapper = new XmlWrapper();
        Spiele = wrapper.GetSpiele("C:\\Spiel.xml");
    }
    
    public String getSpiele()
    {
        return this.Spiele;
    }
}
