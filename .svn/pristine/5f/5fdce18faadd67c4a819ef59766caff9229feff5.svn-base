.seat {
    width: 32px;
    height: 32px;
    background-color: #162a4f;
    float: left;
    margin-right: 10px;
    margin-bottom: 10px;
    color: #fff;
    line-height: 32px;
    text-align: center;
    cursor: pointer;
    position: relative;
}
.seat-occupied {
    width: 32px;
    height: 32px;
    background-color: #ff0000;
    float: left;
    margin-right: 10px;
    margin-bottom: 10px;
    color: #fff;
    line-height: 32px;
    text-align: center;
    cursor: pointer;
    position: relative;
}
.seat:hover {
    background-color: #162a40;
}
a:link {
    color: #fff;
}